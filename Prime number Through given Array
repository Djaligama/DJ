package arrysUsing;

public class PrimeArray {

	public static void main(String[] args) 
	{
		int a[]= {10,20,2,3,11,5,7};
		
		for(int i=0;i<a.length;i++)
		{
			//System.out.println(a[i]);
			int count=0;
			for(int j=0;j<a.length;j++)
			{
				for(int k=1;k<a[i];k++)
				{
					if(a[i]%k==0)
						count++;
				}
				if(count==2)
					System.out.println(a[i]+" prime");
			}
		}
		

	}
}
